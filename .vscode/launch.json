{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Concrete Backup GUI (Direct Python)",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/backup_gui.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Concrete Backup GUI (Normal Mode Script)",
            "type": "node-terminal",
            "request": "launch",
            "command": "./run_backup_gui.sh",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Concrete Backup GUI (Dev Mode Script)",
            "type": "node-terminal",
            "request": "launch",
            "command": "./run_backup_gui_dev.sh",
            "cwd": "${workspaceFolder}"
        }
    ]
}