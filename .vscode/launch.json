    "version": "0.2.0",
    "configurations": [
        {
            "name": "Concrete Backup GUI (Normal Mode Script)",
            "type": "node-terminal",
            "request": "launch",
            "command": "./run_backup_gui.sh",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Concrete Backup GUI (Dev Mode Script)",
            "type": "node-terminal",
            "request": "launch",
            "command": "./run_backup_gui_dev.sh",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Extract Translations",
            "type": "node-terminal",
            "request": "launch",
            "command": "python localization/manage_translations.py extract & exit",
            "cwd": "${workspaceFolder}"
        },
        {
            "name": "Compile Translations",
            "type": "node-terminal",
            "request": "launch",
            "command": "python localization/manage_translations.py compile & exit",
            "cwd": "${workspaceFolder}"
        }
    ]
}